{
  "name": "shooter",
  "version": "0.0.1",
  "description": "game",
  "main": "index.js",
  "scripts": {
    "upload": "aws s3 sync build/ s3://aws-website-blaster-2d3g4 --delete --profile derduher --cache-control 'public, max-age=31540000' --exclude '*.html'",
    "upload-html": "aws s3 sync build/ s3://aws-website-blaster-2d3g4 --profile derduher --cache-control 'public, max-age=600' --exclude '*' --include '*.html'",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "watch": "webpack --progress --watch --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "tsc && jest && codecov --token=9530801e-4ebe-4249-b3db-28762b62eda1"
  },
  "repository": {
    "type": "git"
  },
  "author": "Patrick Weygand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/derduher/blaster/issues"
  },
  "homepage": "https://nimblerendition.com",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.12",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-minify": "^0.5.0",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jasmine": "^3.4.0",
    "jest": "^24.8.0",
    "jest-canvas-mock": "^2.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "source-map": "~0.7.3",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "standard": {
    "globals": [
      "Path2D",
      "describe",
      "xdescribe",
      "it",
      "xit",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "fetch",
      "Event",
      "browser",
      "FocusEvent",
      "FormData",
      "URL",
      "jest",
      "browser"
    ],
    "ignore": [],
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  },
  "dependencies": {
    "codecov": "^3.5.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "jasmine": true,
      "jest": true,
      "mocha": true,
      "node": true
    },
    "rules": {
      "no-case-declarations": 0,
      "no-console": 0,
      "no-unused-vars": 0,
      "react/prop-types": 0
    }
  },
  "jest": {
    "collectCoverage": true,
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "collectCoverageFrom": [
      "src/js/**/*.{ts,tsx}",
      "!node_modules/",
      "!src/js/**/*.spec.*",
      "!src/js/types.ts"
    ]
  }
}
