{
  "name": "shooter",
  "version": "0.0.1",
  "description": "game",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build/*",
    "build": "cp -R src/* build/ & cp node_modules/babel-core/browser-polyfill.js build/js/vendor/browser-polyfill.min.js & npm run build:jade & npm run build:js & npm run build:less & wait",
    "build:js": "browserify src/js/main.js -t [ babelify --blacklist es6.forOf ] --outfile build/js/main.js",
    "build:jade": "jade src/*.jade -o build/",
    "build:less": "lessc --source-map-map-inline src/css/main.less > build/css/main.css",
    "prepublish": "npm run build",
    "watch:jade": "nodemon --watch src -e jade --exec 'npm run build:jade'",
    "watch:less": "nodemon --watch src/css -e less --exec 'npm run build:less'",
    "watch:js": "nodemon --watch src -e js --exec 'npm run build:js'",
    "watch": "npm run watch:jade & npm run watch:js & npm run watch:less",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git"
  },
  "author": "Patrick Weygand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/derduher/flaming-octo-meme/issues"
  },
  "homepage": "https://github.com/derduher/flaming-octo-meme",
  "devDependencies": {
    "babelify": "^6.2.0",
    "jade": "~1.9.2",
    "less": "^2.5.0",
    "nodemon": "^1.3.7"
  },
  "dependencies": {
    "babel-core": "^5.8.22"
  }
}
